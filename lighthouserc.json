{
  "ci": {
    "collect": {
      "staticDistDir": "./dist",
      "url": [
        "/index.html"
      ],
      "numberOfRuns": 1
    },
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "network-dependency-tree-insight": ["warn", { "minScore": 0 }]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
